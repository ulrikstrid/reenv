{
  "name": "reenv",
  "version": "0.3.0",
  "description": "dotenv-cli written in reason",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "bin": [
        "reenv"
      ]
    }
  },
  "scripts": {
    "test": "esy dune build @runtest",
    "doc": "esy dune build @doc",
    "format": "esy dune build @fmt --auto-promote"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/dune": "^2.6.0",
    "@opam/cmdliner": "*",
    "@opam/re": "*",
    "ocaml": "^4.8.0"
  },
  "devDependencies": {
    "@opam/ocamlformat": "*",
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam",
    "@opam/odoc": "*",
    "@reason-native/rely": "*",
    "refmterr": "*"
  }
}
